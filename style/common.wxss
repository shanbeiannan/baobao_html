  font-weight: bolder;
}

.HJ-cell-add-icon image {
  width: 70rpx;
  height: 70rpx;
}

.HJ-cell-add-text {
  margin-top: 20rpx;
  width: 100%;
  text-align: center;
  color: #999;
  font-size: 30rpx;
}

.HJ-cells-uploader {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.HJ-cell-area-min-title {
  margin: 20rpx 0 0 20rpx;
  color: #999;  
  width: 100%;
}

.HJ-cell-area-min {
  background-color: #f8f8f8;
  height: 200rpx;
