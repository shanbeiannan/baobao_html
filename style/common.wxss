/***物品选择页面***/
.HJ-container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  padding: 0 0 25rpx;
}

.HJ-list-left view {
  padding: 25rpx;
  font-size:28rpx;
}

.HJ-list-left view.active {
  background-color: #fff;
  color: #21c179;
}

.HJ-list-left view:active {
  background-color: #fff;
  color: #21c179;
}

.HJ-list-right {
  padding: 25rpx;
  background-color: #fff;
  overflow:hidden;
}

.HJ-product {
  padding: 25rpx 0;
}

.HJ-product-list {
  display: inline-flex;
  align-items: center;
  margin-bottom: 25rpx;
}

.HJ-product-text {
  position:relative;
  text-align: center;
  font-size: 24rpx;
  padding: 15rpx 25rpx;
  color: #333;
  background-color: #e5e5e5;
  border-radius: 15rpx;
  margin-right: 15rpx;
}

.HJ-product-text.active {
  color: #21c179;
  background-color: #d6f4e7;
}
.HJ-product-text.active .content-tip, .HJ-product-text.active .content-tip-img{
  display:block;
}
.content-tip {
  display:none;
  position: absolute;
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 22rpx 22rpx 22rpx 22rpx;
  border-color:  #21c179 #21c179 transparent transparent;
  border-top-right-radius: 15rpx;  
  top:0;
  right:0;
  z-index: 10;
}
.content-tip-img{
  display:none;
  position: absolute;
  z-index:11;
  width:22rpx;
  height:22rpx;
  top:4rpx;
  right:5rpx;
}

.HJ-product-dec {
  flex: 1;
  display: inline-flex;
  align-items: center;
}

.HJ-product-dec input{
  background-color:#f8f8f8;
  padding:10rpx 20rpx;
  border-radius:10rpx;
  margin-right:10rpx;
  color:#21c179;
  font-size:24rpx;
}
.HJ-product-dec text{
  font-size:24rpx;

}
.HJ-product-dec .input-placeholder{
  font-size:24rpx;
}

.HJ-btn{
  background-color:#21c179;
  padding:25rpx;
  width:100% !important;
  text-align: center;
  position: fixed;
  bottom:0;
  color:#fff;
  font-weight: bold; 
   border-radius: 0;
  z-index:100;
}

