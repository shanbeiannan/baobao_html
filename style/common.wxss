/**from**/

.HJ-cells {
  font-family: PingFang-SC-Bold;
  width: 90%;
  display: inline-flex;
  padding: 25rpx 0;
  align-items: center;
  font-size: 32rpx;
}

.HJ-cells-center {
  font-family: PingFang-SC-Bold;
  padding: 20rpx 0;
  display: inline-flex;
  align-items: center;
  justify-items: flex-start;
}

.HJ-cell-title {
  color: #333;
  margin-right: 20rpx;
  font-weight: bolder;
}

.HJ-cell-text {
  text-align: right;
  margin-right: 20rpx;
}

.HJ-cell-right {
  width: 40rpx;
  height: 40rpx;
}

.HJ-cell-right image {
  width: 40rpx;
  height: 40rpx;
}

.HJ-cell-area {
  background-color: #f8f8f8;
  height: 300rpx;
  width: 100%;
  border-radius: 20rpx;
}

.HJ-cell-add {
  position: absolute;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.HJ-cell-add-icon {

  border-radius: 50%;
  line-height: 50rpx;
  width: 70rpx;
  height: 70rpx;
  align-content: center;
  text-align: center; 
  font-weight: bolder;
}

.HJ-cell-add-icon image {
   width: 70rpx;
   height: 70rpx;
}


.HJ-cell-add-text {
  margin-top: 20rpx;
  width: 100%;
  text-align: center;
  color: #999;
  font-size: 30rpx;
}

.HJ-cells-uploader{
  display:flex;
  flex-direction: column;
  justify-content: center;
}
.HJ-cell-area-min-title{
  margin:20rpx 0 0 20rpx;
  color:#999999;
  text-align: center;
  width:100%;
}
.HJ-cell-area-min {
  background-color: #f8f8f8;
  height: 200rpx;
  width: 200rpx;
  border-radius: 20rpx;
  margin: auto 20rpx;
}

.HJ-cell-area-min image.img {
  height: 200rpx;
  width: 200rpx;
}
