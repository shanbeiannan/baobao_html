/***订单页***/
.HJ-cells {
  font-family: PingFang-SC-Bold;
  width: 90%;
  display: inline-flex;
  padding: 25rpx 0;
  align-items: flex-start;
  font-size: 32rpx;
}

.HJ-cells-center {
  font-family: PingFang-SC-Bold;
  padding: 20rpx 0;
  display: inline-flex;
  align-items: center;
  justify-items: flex-start;
}

.HJ-cell-title {
  color: #333;
  margin-right: 20rpx;
  font-weight: bolder;
}

.HJ-cell-dec{
  width:100%;
  padding:20rpx;
  font-size:32rpx;
  color:#666;
  border-radius:20rpx;
}

.HJ-cell-text {
  text-align: right;
  margin-right: 20rpx;
}

.HJ-cell-left,.HJ-cell-right {
  width: 40rpx;
  height: 40rpx;
}

.HJ-cell-left image,.HJ-cell-right image {
  width: 40rpx;
  height: 40rpx;
}
.HJ-inputView{/*模仿inputplaceholder*/
  font-size:32rpx;
  color:#999;
}
.HJ-cell-area {
  background-color: #f8f8f8;
  height: 300rpx;
  width: 100%;
  border-radius: 20rpx;
}

.HJ-cell-list {
  width: 100%;
  display: flex;
  flex-direction: flex;
  background-color: #f8f8f8;
  padding: 20rpx;
  font-size: 30rpx;
  color: #666;
  border-radius: 20rpx;
}

.HJ-cell-add {
  position: absolute;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-self: center;
  align-items: center;
}

.HJ-cell-add-icon {
  border-radius: 50%;
  line-height: 50rpx;
  width: 70rpx;
  height: 70rpx;
  align-content: center;
